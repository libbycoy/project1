<div class="uk-block uk-block uk-width-1-1">
<h1>Sign in</h1></div>

<%= form_tag login_path, :class => 'uk-form' do %>
  <%= label_tag :email %>
  <%= email_field_tag :email, nil, :placeholder => 'littledouggie@gmail.com' %>

  <%= label_tag :password %>
  <%= password_field_tag :password %>

  <%= submit_tag 'Log in', class: 'uk-button' %>
<% end %>
